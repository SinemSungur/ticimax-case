{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.reduce.js\";\nimport { DEFAULT_VALUES, TABLE_COLUMN_KEYS } from '@/constants/content';\nexport default {\n  name: 'BaseTable',\n  data() {\n    return {\n      DEFAULT_VALUES,\n      TABLE_COLUMN_KEYS\n    };\n  },\n  props: {\n    data: {\n      type: Array,\n      default: () => []\n    },\n    tableColumnData: {\n      type: Array,\n      default: () => []\n    }\n  },\n  methods: {\n    getColStyle(field) {\n      const width = field.width;\n      if (width) return `width: ${width}px; min-width: ${width}px;`;\n      return undefined;\n    },\n    handleImageError(event) {\n      // Remove image source to show empty area\n      event.target.style.display = 'none';\n    },\n    truncateText(text, maxLength) {\n      if (!text) return '';\n      return text.length > maxLength ? text.substring(0, maxLength) + '...' : text;\n    },\n    formatPrice(price) {\n      if (!price && price !== 0) return '';\n      return `${DEFAULT_VALUES.CURRENCY_SYMBOL}${price.toFixed(2)}`;\n    },\n    formatDiscount(discount) {\n      if (!discount && discount !== 0) return '';\n      return `${DEFAULT_VALUES.PERCENTAGE_SYMBOL}${discount}`;\n    },\n    formatAvailability(status) {\n      if (!status) return 'Unknown';\n      return status === 'In Stock' ? 'In Stock' : 'Out of Stock';\n    },\n    getAvailabilityClass(status) {\n      if (!status) return 'availability-unknown';\n      return status === 'In Stock' ? 'availability-in-stock' : 'availability-out-of-stock';\n    },\n    formatDimensions(dimensions) {\n      if (!dimensions) return '';\n      const {\n        width,\n        height,\n        depth\n      } = dimensions;\n      if (!width || !height || !depth) return '';\n      return `${width}${DEFAULT_VALUES.DIMENSION_SEPARATOR}${height}${DEFAULT_VALUES.DIMENSION_SEPARATOR}${depth}`;\n    },\n    formatWeight(weight) {\n      if (!weight && weight !== 0) return '';\n      return `${weight}${DEFAULT_VALUES.WEIGHT_UNIT}`;\n    },\n    formatDate(dateString) {\n      if (!dateString) return '';\n      const date = new Date(dateString);\n      if (isNaN(date.getTime())) return '';\n      return date.toLocaleDateString('en-US', {\n        year: 'numeric',\n        month: 'short',\n        day: 'numeric',\n        hour: '2-digit',\n        minute: '2-digit'\n      });\n    },\n    getNestedValue(obj, path) {\n      if (!obj || !path) return '';\n      return path.split('.').reduce((current, key) => {\n        return current && current[key] !== undefined ? current[key] : '';\n      }, obj);\n    },\n    getRatingComment(item) {\n      if (!item || !item.reviews || !item.reviews.length) return 'No reviews';\n\n      // Get the first review comment or a default message\n      const firstReview = item.reviews[0];\n      if (firstReview && firstReview.comment) {\n        return firstReview.comment;\n      }\n      return 'No comment available';\n    },\n    capitalizeFirstLetter(string) {\n      if (!string) return '';\n      return string.charAt(0).toUpperCase() + string.slice(1);\n    }\n  }\n};","map":{"version":3,"names":["DEFAULT_VALUES","TABLE_COLUMN_KEYS","name","data","props","type","Array","default","tableColumnData","methods","getColStyle","field","width","undefined","handleImageError","event","target","style","display","truncateText","text","maxLength","length","substring","formatPrice","price","CURRENCY_SYMBOL","toFixed","formatDiscount","discount","PERCENTAGE_SYMBOL","formatAvailability","status","getAvailabilityClass","formatDimensions","dimensions","height","depth","DIMENSION_SEPARATOR","formatWeight","weight","WEIGHT_UNIT","formatDate","dateString","date","Date","isNaN","getTime","toLocaleDateString","year","month","day","hour","minute","getNestedValue","obj","path","split","reduce","current","key","getRatingComment","item","reviews","firstReview","comment","capitalizeFirstLetter","string","charAt","toUpperCase","slice"],"sources":["src/components/atoms/BaseTable.vue"],"sourcesContent":["<template>\n  <div class=\"table-wrapper\">\n    <div ref=\"tableBody\" class=\"table-main fixed-header\">\n      <table>\n        <colgroup>\n          <col :style=\"getColStyle(field)\" v-for=\"(field, index) in tableColumnData\" :key=\"index\" />\n        </colgroup>\n        <thead class=\"table-head\">\n          <tr>\n            <th v-for=\"(field, index) in tableColumnData\" :key=\"index\">\n              {{ field.name }}\n            </th>\n          </tr>\n        </thead>\n        <tbody class=\"table-body\">\n          <tr v-for=\"(item, index) in data\" :key=\"item.id || index\">\n            <td v-for=\"(field, fieldIndex) in tableColumnData\" :key=\"fieldIndex\">\n              <span>\n                <!-- Image Column -->\n                <div v-if=\"!!field.scopedSlots && field.scopedSlots.customRender === 'imageAction'\">\n                  <div class=\"image-container\">\n                    <img \n                      :src=\"item[field.key]\" \n                      :alt=\"item.title || DEFAULT_VALUES.PRODUCT_IMAGE_ALT\"\n                      class=\"product-image\"\n                      @error=\"handleImageError\"\n                    />\n                  </div>\n                </div>\n                \n                <!-- Description Column -->\n                <div v-else-if=\"!!field.scopedSlots && field.scopedSlots.customRender === 'descriptionAction'\">\n                  <div class=\"description-text\">\n                    {{ truncateText(item[field.key], DEFAULT_VALUES.DESCRIPTION_MAX_LENGTH) }}\n                  </div>\n                </div>\n                \n                <!-- Price Column -->\n                <div v-else-if=\"!!field.scopedSlots && field.scopedSlots.customRender === 'priceAction'\">\n                  <div class=\"price-text\">\n                    {{ formatPrice(item[field.key]) }}\n                  </div>\n                </div>\n                \n                <!-- Discount Column -->\n                <div v-else-if=\"!!field.scopedSlots && field.scopedSlots.customRender === 'discountAction'\">\n                  <div class=\"discount-text\">\n                    {{ formatDiscount(item[field.key]) }}\n                  </div>\n                </div>\n                \n                <!-- Availability Column -->\n                <div v-else-if=\"!!field.scopedSlots && field.scopedSlots.customRender === 'availabilityAction'\">\n                  <div class=\"availability-badge\" :class=\"getAvailabilityClass(item[field.key])\">\n                    {{ formatAvailability(item[field.key]) }}\n                  </div>\n                </div>\n                \n                <!-- Rating Column -->\n                <div v-else-if=\"!!field.scopedSlots && field.scopedSlots.customRender === 'ratingAction'\">\n                  <div class=\"rating-container\">\n                    <span class=\"rating-value\">{{ item[field.key] }}</span>\n                    <span class=\"rating-comment\">{{ getRatingComment(item) }}</span>\n                  </div>\n                </div>\n                \n                <!-- Date Column -->\n                <div v-else-if=\"!!field.scopedSlots && field.scopedSlots.customRender === 'dateAction'\">\n                  <div class=\"date-text\">\n                    {{ formatDate(item[field.key]) }}\n                  </div>\n                </div>\n                \n                <!-- Size Column -->\n                <div v-else-if=\"field.key === TABLE_COLUMN_KEYS.DIMENSIONS\">\n                  <div class=\"size-text\">\n                    {{ formatDimensions(item[field.key]) }}\n                  </div>\n                </div>\n                \n                <!-- Weight Column -->\n                <div v-else-if=\"field.key === TABLE_COLUMN_KEYS.WEIGHT\">\n                  <div class=\"weight-text\">\n                    {{ formatWeight(item[field.key]) }}\n                  </div>\n                </div>\n                \n                <!-- Barcode Column -->\n                <div v-else-if=\"field.key === TABLE_COLUMN_KEYS.BARCODE\">\n                  <div class=\"barcode-text\">\n                    {{ getNestedValue(item, field.key) }}\n                  </div>\n                </div>\n                \n                <!-- Category Column -->\n                <div v-else-if=\"field.key === TABLE_COLUMN_KEYS.CATEGORY\">\n                  <div class=\"category-text\">\n                    {{ capitalizeFirstLetter(item[field.key]) }}\n                  </div>\n                </div>\n                \n                <!-- Last Updated Column -->\n                <div v-else-if=\"field.key === TABLE_COLUMN_KEYS.UPDATED_AT\">\n                  <div class=\"date-text\">\n                    {{ formatDate(getNestedValue(item, field.key)) }}\n                  </div>\n                </div>\n                \n                <!-- Default Column -->\n                <span v-else> {{ getNestedValue(item, field.key) || '' }}</span>\n              </span>\n            </td>\n          </tr>\n        </tbody>\n      </table>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { \n  DEFAULT_VALUES, \n  TABLE_COLUMN_KEYS \n} from '@/constants/content'\n\nexport default {\n  name: 'BaseTable',\n  data() {\n    return {\n      DEFAULT_VALUES,\n      TABLE_COLUMN_KEYS\n    }\n  },\n  props: {\n    data: {\n      type: Array,\n      default: () => []\n    },\n    tableColumnData: {\n      type: Array,\n      default: () => []\n    }\n  },\n  methods: {\n    getColStyle(field) {\n      const width = field.width\n      if (width) return `width: ${width}px; min-width: ${width}px;`\n      return undefined\n    },\n    handleImageError(event) {\n      // Remove image source to show empty area\n      event.target.style.display = 'none'\n    },\n    truncateText(text, maxLength) {\n      if (!text) return ''\n      return text.length > maxLength ? text.substring(0, maxLength) + '...' : text\n    },\n    formatPrice(price) {\n      if (!price && price !== 0) return ''\n      return `${DEFAULT_VALUES.CURRENCY_SYMBOL}${price.toFixed(2)}`\n    },\n    formatDiscount(discount) {\n      if (!discount && discount !== 0) return ''\n      return `${DEFAULT_VALUES.PERCENTAGE_SYMBOL}${discount}`\n    },\n    formatAvailability(status) {\n      if (!status) return 'Unknown'\n      return status === 'In Stock' ? 'In Stock' : 'Out of Stock'\n    },\n    getAvailabilityClass(status) {\n      if (!status) return 'availability-unknown'\n      return status === 'In Stock' ? 'availability-in-stock' : 'availability-out-of-stock'\n    },\n    formatDimensions(dimensions) {\n      if (!dimensions) return ''\n      const { width, height, depth } = dimensions\n      if (!width || !height || !depth) return ''\n      return `${width}${DEFAULT_VALUES.DIMENSION_SEPARATOR}${height}${DEFAULT_VALUES.DIMENSION_SEPARATOR}${depth}`\n    },\n    formatWeight(weight) {\n      if (!weight && weight !== 0) return ''\n      return `${weight}${DEFAULT_VALUES.WEIGHT_UNIT}`\n    },\n    formatDate(dateString) {\n      if (!dateString) return ''\n      const date = new Date(dateString)\n      if (isNaN(date.getTime())) return ''\n      return date.toLocaleDateString('en-US', {\n        year: 'numeric',\n        month: 'short',\n        day: 'numeric',\n        hour: '2-digit',\n        minute: '2-digit'\n      })\n    },\n    getNestedValue(obj, path) {\n      if (!obj || !path) return ''\n      return path.split('.').reduce((current, key) => {\n        return current && current[key] !== undefined ? current[key] : ''\n      }, obj)\n    },\n    getRatingComment(item) {\n      if (!item || !item.reviews || !item.reviews.length) return 'No reviews'\n      \n      // Get the first review comment or a default message\n      const firstReview = item.reviews[0]\n      if (firstReview && firstReview.comment) {\n        return firstReview.comment\n      }\n      \n      return 'No comment available'\n    },\n    capitalizeFirstLetter(string) {\n      if (!string) return ''\n      return string.charAt(0).toUpperCase() + string.slice(1)\n    }\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n.table-wrapper {\n  width: 100%;\n  margin-right: auto;\n  margin-left: auto;\n  min-height: fit-content;\n\n  table {\n    border: 1px solid #dadada;\n    border-radius: 8px 8px 0px 0px;\n    table-layout: fixed;\n    width: 100%;\n    margin: 0 auto;\n  }\n\n  .table-main {\n    border: none;\n    width: 100%;\n    overflow: auto;\n    &.fixed-header {\n      th {\n        position: sticky;\n        top: 0;\n        z-index: 2;\n      }\n    }\n    & .table-head {\n      font-style: normal;\n      font-weight: 500;\n      font-size: 14px;\n      line-height: 17px;\n      width: 100%;\n      white-space: nowrap;\n      position: relative;\n      text-align: left;\n      & tr {\n        & th {\n          background: #f5f5f5;\n          width: fit-content;\n          padding: 16px;\n          font-weight: 600;\n          border-bottom: 1px solid #dadada;\n          border-right: 1px solid #dadada;\n          &:first-child {\n            border-radius: 8px 0px 0px 0px;\n          }\n          &:last-child {\n            border-radius: 0px 8px 0px 0px;\n            border-right: none;\n          }\n        }\n      }\n    }\n    & .table-body {\n      & tr {\n        padding: 16px;\n        position: relative;\n\n        & td {\n          border-right: 1px solid #dadada;\n          &:last-child {\n            border-right: none;\n          }\n        }\n        &:hover {\n          cursor: pointer;\n        }\n      }\n    }\n    & td {\n      border-bottom: 1px solid #dadada;\n      background: #ffffff;\n      text-align: left;\n      font-style: normal;\n      font-weight: 500;\n      font-size: 14px;\n      color: #484848;\n      line-height: 17px;\n      padding: 16px;\n      vertical-align: middle;\n      & span {\n        display: flex;\n        flex-direction: row;\n        align-items: center;\n        gap: 5px;\n      }\n      & .image-container {\n        display: flex;\n        justify-content: center;\n        align-items: center;\n        width: 100%;\n        height: 80px;\n        & .product-image {\n          max-width: 100%;\n          max-height: 100%;\n          object-fit: contain;\n          border-radius: 4px;\n          border: 1px solid #e0e0e0;\n          background-color: #f9f9f9;\n          transition: transform 0.2s ease;\n          &:hover {\n            transform: scale(1.05);\n          }\n        }\n      }\n      \n      & .description-text {\n        max-width: 100%;\n        word-wrap: break-word;\n        line-height: 1.4;\n      }\n      \n      & .price-text {\n        font-weight: 600;\n        color: #2c3e50;\n      }\n      \n      & .discount-text {\n        font-weight: 600;\n        color: #e74c3c;\n      }\n      \n      & .availability-badge {\n        padding: 4px 8px;\n        border-radius: 12px;\n        font-size: 12px;\n        font-weight: 600;\n        text-align: center;\n        min-width: 80px;\n        \n        &.availability-in-stock {\n          background-color: #d4edda;\n          color: #155724;\n        }\n        \n        &.availability-out-of-stock {\n          background-color: #f8d7da;\n          color: #721c24;\n        }\n        \n        &.availability-unknown {\n          background-color: #f8f9fa;\n          color: #6c757d;\n        }\n      }\n      \n      & .rating-container {\n        display: flex;\n        flex-direction: column;\n        align-items: flex-start;\n        gap: 4px;\n        \n        .rating-value {\n          font-weight: 600;\n          color: #2c3e50;\n          font-size: 14px;\n        }\n        \n        .rating-comment {\n          font-size: 11px;\n          color: #6c757d;\n          font-style: italic;\n        }\n      }\n      \n      & .date-text {\n        font-size: 13px;\n        color: #6c757d;\n      }\n      \n      & .size-text,\n      & .weight-text,\n      & .barcode-text {\n        font-family: 'Courier New', monospace;\n        font-size: 13px;\n      }\n      \n      & .category-text {\n        font-weight: 500;\n        color: #2c3e50;\n        text-transform: capitalize;\n      }\n    }\n  }\n}\n</style> "],"mappings":";;AAwHA,SACAA,cAAA,EACAC,iBAAA,QACA;AAEA;EACAC,IAAA;EACAC,KAAA;IACA;MACAH,cAAA;MACAC;IACA;EACA;EACAG,KAAA;IACAD,IAAA;MACAE,IAAA,EAAAC,KAAA;MACAC,OAAA,EAAAA,CAAA;IACA;IACAC,eAAA;MACAH,IAAA,EAAAC,KAAA;MACAC,OAAA,EAAAA,CAAA;IACA;EACA;EACAE,OAAA;IACAC,YAAAC,KAAA;MACA,MAAAC,KAAA,GAAAD,KAAA,CAAAC,KAAA;MACA,IAAAA,KAAA,mBAAAA,KAAA,kBAAAA,KAAA;MACA,OAAAC,SAAA;IACA;IACAC,iBAAAC,KAAA;MACA;MACAA,KAAA,CAAAC,MAAA,CAAAC,KAAA,CAAAC,OAAA;IACA;IACAC,aAAAC,IAAA,EAAAC,SAAA;MACA,KAAAD,IAAA;MACA,OAAAA,IAAA,CAAAE,MAAA,GAAAD,SAAA,GAAAD,IAAA,CAAAG,SAAA,IAAAF,SAAA,YAAAD,IAAA;IACA;IACAI,YAAAC,KAAA;MACA,KAAAA,KAAA,IAAAA,KAAA;MACA,UAAAzB,cAAA,CAAA0B,eAAA,GAAAD,KAAA,CAAAE,OAAA;IACA;IACAC,eAAAC,QAAA;MACA,KAAAA,QAAA,IAAAA,QAAA;MACA,UAAA7B,cAAA,CAAA8B,iBAAA,GAAAD,QAAA;IACA;IACAE,mBAAAC,MAAA;MACA,KAAAA,MAAA;MACA,OAAAA,MAAA;IACA;IACAC,qBAAAD,MAAA;MACA,KAAAA,MAAA;MACA,OAAAA,MAAA;IACA;IACAE,iBAAAC,UAAA;MACA,KAAAA,UAAA;MACA;QAAAvB,KAAA;QAAAwB,MAAA;QAAAC;MAAA,IAAAF,UAAA;MACA,KAAAvB,KAAA,KAAAwB,MAAA,KAAAC,KAAA;MACA,UAAAzB,KAAA,GAAAZ,cAAA,CAAAsC,mBAAA,GAAAF,MAAA,GAAApC,cAAA,CAAAsC,mBAAA,GAAAD,KAAA;IACA;IACAE,aAAAC,MAAA;MACA,KAAAA,MAAA,IAAAA,MAAA;MACA,UAAAA,MAAA,GAAAxC,cAAA,CAAAyC,WAAA;IACA;IACAC,WAAAC,UAAA;MACA,KAAAA,UAAA;MACA,MAAAC,IAAA,OAAAC,IAAA,CAAAF,UAAA;MACA,IAAAG,KAAA,CAAAF,IAAA,CAAAG,OAAA;MACA,OAAAH,IAAA,CAAAI,kBAAA;QACAC,IAAA;QACAC,KAAA;QACAC,GAAA;QACAC,IAAA;QACAC,MAAA;MACA;IACA;IACAC,eAAAC,GAAA,EAAAC,IAAA;MACA,KAAAD,GAAA,KAAAC,IAAA;MACA,OAAAA,IAAA,CAAAC,KAAA,MAAAC,MAAA,EAAAC,OAAA,EAAAC,GAAA;QACA,OAAAD,OAAA,IAAAA,OAAA,CAAAC,GAAA,MAAA/C,SAAA,GAAA8C,OAAA,CAAAC,GAAA;MACA,GAAAL,GAAA;IACA;IACAM,iBAAAC,IAAA;MACA,KAAAA,IAAA,KAAAA,IAAA