{"ast":null,"code":"import { BaseTable, BaseSpinner, BaseButton, SortControl, Pagination } from '@/components';\nimport { ERROR_MESSAGES, BUTTON_LABELS, TABLE_COLUMNS, TABLE_COLUMN_KEYS, TABLE_COLUMN_WIDTHS, SCOPED_SLOT_TYPES } from '@/constants/content';\nexport default {\n  name: 'ProductGrid',\n  components: {\n    BaseTable,\n    BaseSpinner,\n    BaseButton,\n    SortControl,\n    Pagination\n  },\n  data() {\n    return {\n      ERROR_MESSAGES,\n      BUTTON_LABELS,\n      tableColumns: [{\n        name: TABLE_COLUMNS.IMAGE,\n        width: TABLE_COLUMN_WIDTHS.IMAGE,\n        key: TABLE_COLUMN_KEYS.THUMBNAIL,\n        scopedSlots: {\n          customRender: SCOPED_SLOT_TYPES.IMAGE_ACTION\n        }\n      }, {\n        name: TABLE_COLUMNS.PRODUCT_NAME,\n        width: TABLE_COLUMN_WIDTHS.PRODUCT_NAME,\n        key: TABLE_COLUMN_KEYS.TITLE\n      }, {\n        name: TABLE_COLUMNS.PRICE,\n        width: TABLE_COLUMN_WIDTHS.PRICE,\n        key: TABLE_COLUMN_KEYS.PRICE\n      }, {\n        name: TABLE_COLUMNS.CATEGORY,\n        width: TABLE_COLUMN_WIDTHS.CATEGORY,\n        key: TABLE_COLUMN_KEYS.CATEGORY\n      }, {\n        name: TABLE_COLUMNS.BRAND,\n        width: TABLE_COLUMN_WIDTHS.BRAND,\n        key: TABLE_COLUMN_KEYS.BRAND\n      }, {\n        name: TABLE_COLUMNS.STOCK,\n        width: TABLE_COLUMN_WIDTHS.STOCK,\n        key: TABLE_COLUMN_KEYS.STOCK\n      }, {\n        name: TABLE_COLUMNS.DISCOUNT,\n        width: TABLE_COLUMN_WIDTHS.DISCOUNT,\n        key: TABLE_COLUMN_KEYS.DISCOUNT_PERCENTAGE\n      }, {\n        name: TABLE_COLUMNS.RATING,\n        width: TABLE_COLUMN_WIDTHS.RATING,\n        key: TABLE_COLUMN_KEYS.RATING\n      }]\n    };\n  },\n  props: {\n    products: {\n      type: Array,\n      default: () => []\n    },\n    loading: {\n      type: Boolean,\n      default: false\n    },\n    error: {\n      type: String,\n      default: null\n    },\n    currentPage: {\n      type: Number,\n      required: true\n    },\n    totalPages: {\n      type: Number,\n      required: true\n    },\n    currentSort: {\n      type: String,\n      required: true\n    }\n  }\n};","map":{"version":3,"names":["BaseTable","BaseSpinner","BaseButton","SortControl","Pagination","ERROR_MESSAGES","BUTTON_LABELS","TABLE_COLUMNS","TABLE_COLUMN_KEYS","TABLE_COLUMN_WIDTHS","SCOPED_SLOT_TYPES","name","components","data","tableColumns","IMAGE","width","key","THUMBNAIL","scopedSlots","customRender","IMAGE_ACTION","PRODUCT_NAME","TITLE","PRICE","CATEGORY","BRAND","STOCK","DISCOUNT","DISCOUNT_PERCENTAGE","RATING","props","products","type","Array","default","loading","Boolean","error","String","currentPage","Number","required","totalPages","currentSort"],"sources":["src/components/organisms/ProductGrid.vue"],"sourcesContent":["<template>\n    <div class=\"product-grid\">\n        <div v-if=\"loading\" class=\"product-grid__loading\">\n            <BaseSpinner />\n        </div>\n\n        <div v-else-if=\"error\" class=\"product-grid__error\">\n            <div class=\"error-message\">\n                <h3>{{ ERROR_MESSAGES.GENERAL_ERROR }}</h3>\n                <p>{{ error }}</p>\n                <BaseButton @click=\"$emit('retry')\">{{ BUTTON_LABELS.TRY_AGAIN }}</BaseButton>\n            </div>\n        </div>\n\n        <div v-else-if=\"products.length === 0\" class=\"product-grid__empty\">\n            <div class=\"empty-message\">\n                <h3>{{ ERROR_MESSAGES.NO_PRODUCTS_FOUND }}</h3>\n                <p>{{ ERROR_MESSAGES.NO_PRODUCTS_AVAILABLE }}</p>\n            </div>\n        </div>\n\n        <div v-else class=\"product-grid__content\">\n            <div class=\"product-grid__sort\">\n                <SortControl \n                    :current-sort=\"currentSort\"\n                    @sort-change=\"$emit('sort-change', $event)\"\n                />\n            </div>\n            \n            <div class=\"product-grid__table\">\n                <BaseTable \n                    :data=\"products\"\n                    :table-column-data=\"tableColumns\"\n                />\n            </div>\n            \n            <div class=\"product-grid__pagination\">\n                <Pagination \n                    :current-page=\"currentPage\"\n                    :total-pages=\"totalPages\"\n                    @page-change=\"$emit('page-change', $event)\"\n                />\n            </div>\n        </div>\n    </div>\n</template>\n\n<script>\nimport { BaseTable, BaseSpinner, BaseButton, SortControl, Pagination } from '@/components'\nimport { \n    ERROR_MESSAGES, \n    BUTTON_LABELS, \n    TABLE_COLUMNS, \n    TABLE_COLUMN_KEYS, \n    TABLE_COLUMN_WIDTHS, \n    SCOPED_SLOT_TYPES \n} from '@/constants/content'\n\nexport default {\n    name: 'ProductGrid',\n    components: {\n        BaseTable,\n        BaseSpinner,\n        BaseButton,\n        SortControl,\n        Pagination\n    },\n    data() {\n        return {\n            ERROR_MESSAGES,\n            BUTTON_LABELS,\n            tableColumns: [\n                {\n                    name: TABLE_COLUMNS.IMAGE,\n                    width: TABLE_COLUMN_WIDTHS.IMAGE,\n                    key: TABLE_COLUMN_KEYS.THUMBNAIL,\n                    scopedSlots: {\n                        customRender: SCOPED_SLOT_TYPES.IMAGE_ACTION\n                    }\n                },\n                {\n                    name: TABLE_COLUMNS.PRODUCT_NAME,\n                    width: TABLE_COLUMN_WIDTHS.PRODUCT_NAME,\n                    key: TABLE_COLUMN_KEYS.TITLE\n                },\n                {\n                    name: TABLE_COLUMNS.PRICE,\n                    width: TABLE_COLUMN_WIDTHS.PRICE,\n                    key: TABLE_COLUMN_KEYS.PRICE\n                },\n                {\n                    name: TABLE_COLUMNS.CATEGORY,\n                    width: TABLE_COLUMN_WIDTHS.CATEGORY,\n                    key: TABLE_COLUMN_KEYS.CATEGORY\n                },\n                {\n                    name: TABLE_COLUMNS.BRAND,\n                    width: TABLE_COLUMN_WIDTHS.BRAND,\n                    key: TABLE_COLUMN_KEYS.BRAND\n                },\n                {\n                    name: TABLE_COLUMNS.STOCK,\n                    width: TABLE_COLUMN_WIDTHS.STOCK,\n                    key: TABLE_COLUMN_KEYS.STOCK\n                },\n                {\n                    name: TABLE_COLUMNS.DISCOUNT,\n                    width: TABLE_COLUMN_WIDTHS.DISCOUNT,\n                    key: TABLE_COLUMN_KEYS.DISCOUNT_PERCENTAGE\n                },\n                {\n                    name: TABLE_COLUMNS.RATING,\n                    width: TABLE_COLUMN_WIDTHS.RATING,\n                    key: TABLE_COLUMN_KEYS.RATING\n                }\n            ]\n        }\n    },\n    props: {\n        products: {\n            type: Array,\n            default: () => []\n        },\n        loading: {\n            type: Boolean,\n            default: false\n        },\n        error: {\n            type: String,\n            default: null\n        },\n        currentPage: {\n            type: Number,\n            required: true\n        },\n        totalPages: {\n            type: Number,\n            required: true\n        },\n        currentSort: {\n            type: String,\n            required: true\n        }\n    }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n.product-grid {\n    &__loading,\n    &__error,\n    &__empty {\n        display: flex;\n        justify-content: center;\n        align-items: center;\n        min-height: 300px;\n    }\n\n    &__content {\n        width: 100%;\n    }\n\n    &__sort {\n        margin-bottom: 20px;\n        display: flex;\n        justify-content: flex-end;\n        align-items: center;\n        padding: 10px 0;\n        border: 1px solid #e0e0e0;\n        border-radius: 4px;\n        background-color: #f9f9f9;\n        padding: 15px;\n    }\n\n    &__table {\n        width: 100%;\n        margin-bottom: 20px;\n    }\n\n    &__pagination {\n        display: flex;\n        justify-content: center;\n        margin-top: 20px;\n    }\n}\n\n.error-message,\n.empty-message {\n    text-align: center;\n\n    h3 {\n        margin: 0 0 12px 0;\n        color: #2c3e50;\n    }\n\n    p {\n        margin: 0 0 16px 0;\n        color: #6c757d;\n    }\n}\n\n.error-message h3 {\n    color: #dc3545;\n}\n</style>"],"mappings":"AAgDA,SAAAA,SAAA,EAAAC,WAAA,EAAAC,UAAA,EAAAC,WAAA,EAAAC,UAAA;AACA,SACAC,cAAA,EACAC,aAAA,EACAC,aAAA,EACAC,iBAAA,EACAC,mBAAA,EACAC,iBAAA,QACA;AAEA;EACAC,IAAA;EACAC,UAAA;IACAZ,SAAA;IACAC,WAAA;IACAC,UAAA;IACAC,WAAA;IACAC;EACA;EACAS,KAAA;IACA;MACAR,cAAA;MACAC,aAAA;MACAQ,YAAA,GACA;QACAH,IAAA,EAAAJ,aAAA,CAAAQ,KAAA;QACAC,KAAA,EAAAP,mBAAA,CAAAM,KAAA;QACAE,GAAA,EAAAT,iBAAA,CAAAU,SAAA;QACAC,WAAA;UACAC,YAAA,EAAAV,iBAAA,CAAAW;QACA;MACA,GACA;QACAV,IAAA,EAAAJ,aAAA,CAAAe,YAAA;QACAN,KAAA,EAAAP,mBAAA,CAAAa,YAAA;QACAL,GAAA,EAAAT,iBAAA,CAAAe;MACA,GACA;QACAZ,IAAA,EAAAJ,aAAA,CAAAiB,KAAA;QACAR,KAAA,EAAAP,mBAAA,CAAAe,KAAA;QACAP,GAAA,EAAAT,iBAAA,CAAAgB;MACA,GACA;QACAb,IAAA,EAAAJ,aAAA,CAAAkB,QAAA;QACAT,KAAA,EAAAP,mBAAA,CAAAgB,QAAA;QACAR,GAAA,EAAAT,iBAAA,CAAAiB;MACA,GACA;QACAd,IAAA,EAAAJ,aAAA,CAAAmB,KAAA;QACAV,KAAA,EAAAP,mBAAA,CAAAiB,KAAA;QACAT,GAAA,EAAAT,iBAAA,CAAAkB;MACA,GACA;QACAf,IAAA,EAAAJ,aAAA,CAAAoB,KAAA;QACAX,KAAA,EAAAP,mBAAA,CAAAkB,KAAA;QACAV,GAAA,EAAAT,iBAAA,CAAAmB;MACA,GACA;QACAhB,IAAA,EAAAJ,aAAA,CAAAqB,QAAA;QACAZ,KAAA,EAAAP,mBAAA,CAAAmB,QAAA;QACAX,GAAA,EAAAT,iBAAA,CAAAqB;MACA,GACA;QACAlB,IAAA,EAAAJ,aAAA,CAAAuB,MAAA;QACAd,KAAA,EAAAP,mBAAA,CAAAqB,MAAA;QACAb,GAAA,EAAAT,iBAAA,CAAAsB;MACA;IAEA;EACA;EACAC,KAAA;IACAC,QAAA;MACAC,IAAA,EAAAC,KAAA;MACAC,OAAA,EAAAA,CAAA;IACA;IACAC,OAAA;MACAH,IAAA,EAAAI,OAAA;MACAF,OAAA;IACA;IACAG,KAAA;MACAL,IAAA,EAAAM,MAAA;MACAJ,OAAA;IACA;IACAK,WAAA;MACAP,IAAA,EAAAQ,MAAA;MACAC,QAAA;IACA;IACAC,UAAA;MACAV,IAAA,EAAAQ,MAAA;MACAC,QAAA;IACA;IACAE,WAAA;MACAX,IAAA,EAAAM,MAAA;MACAG,QAAA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}